<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jonathant.calendar" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>MT.A.RANGE</name>
    <description>
        A calendar app
    </description>
    <author email="jonathan.turnbull8@gmail.com" href="http://jtpartywolf.com">
        Jonathan Turnbull
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="xhdpi" src="res/icon/android/appicon-xhdpi.png" />
    <splash density="port-hdpi" src="res/screen/android/splash-portrait-hdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
